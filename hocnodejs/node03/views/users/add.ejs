<h1>Them nguoi dung</h1>
<form action="" method="post">
  <div class="mb-3">
    <label for="">Ten</label>
    <input type="text" name="name" class="form-control" placeholder="Ten..." />
  </div>
  <div class="mb-3">
    <label for="">Email</label>
    <input
      type="email"
      name="email"
      class="form-control"
      placeholder="Email..."
    />
  </div>
  <div class="mb-3">
    <label for="">Trang thai</label>
    <select name="status" class="form-select">
      <option value="0">Chua kich hoat</option>
      <option value="1">Kich hoat</option>
    </select>
  </div>
  <button class="btn btn-success">Lưu</button>
  <a href="/users" class="btn btn-danger">Hủy</a>
</form>
